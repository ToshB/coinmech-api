{
  "name": "oslopinball-api",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "Torstein Bj√∏rnstad (tosh@tosh.no)",
  "license": "MIT",
  "scripts": {
    "start": "env $(cat .env) nodemon | pino",
    "tstart": "concurrently 'npm run start' 'npm run tunnel'",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "test": "jest",
    "heroku-postbuild": "PGSSLMODE=require pg-migrate up",
    "tunnel": "lt --port 4000 --subdomain coinmech"
  },
  "engines": {
    "node": "8.2.1"
  },
  "dependencies": {
    "@types/pg-connection-string": "^0.1.1",
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "eventstore": "^1.12.3",
    "express": "^4.15.3",
    "googleapis": "^20.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.4.3",
    "localtunnel": "^1.8.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-pg-migrate": "^2.7.0",
    "pg": "^7.1.0",
    "pg-connection-string": "^0.1.3",
    "pino": "^4.7.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.36",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^20.0.6",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/lodash": "^4.14.71",
    "@types/node-fetch": "^1.6.7",
    "@types/node-pg-migrate": "^2.3.0",
    "@types/pg": "^6.1.43",
    "@types/pino": "^4.7.0",
    "@types/sinon": "^2.3.3",
    "concurrently": "^3.5.0",
    "eslint": "^4.4.1",
    "hippie": "^0.5.1",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "sinon": "^3.2.0",
    "ts-jest": "^20.0.10",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*\\.test)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
